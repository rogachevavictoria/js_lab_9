extends layout
block title
    | Index page
block content
    include ./header
    div.w3-center.w3-padding-32
        h3 Bidding for #{auction["picName"]} starts #{auction["dateBegin"]} at #{auction["timeBegin"]}
        br
        h3 Bidders:
        br
        for par in participants
            for pic in par["auction"]
                if (pic["picture"] == auction["picName"])
                    h4(align="center") #{par["name"]}

